{
  "name": "koa-range-static",
  "version": "1.0.0",
  "description": "Static file server middleware, support for range request",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "main": "./cjs/index.js",
  "types": "./cjs/index.d.ts",
  "scripts": {
    "test": "mocha",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "package": "npm run test && node clean.js && npm run build:cjs && npm run build:esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tarrowren/koa-range-static.git"
  },
  "author": "Tarrowren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tarrowren/koa-range-static/issues"
  },
  "homepage": "https://github.com/Tarrowren/koa-range-static#readme",
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.43",
    "@types/should": "^13.0.0",
    "@types/supertest": "^2.0.12",
    "koa": "^2.13.4",
    "mocha": "^10.0.0",
    "should": "^13.2.3",
    "supertest": "^6.2.4",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
